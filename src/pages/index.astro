---
import Layout from '@/layouts/Layout.astro';
import Header from '@/components/shadcn-studio/blocks/hero-section-01/header';
import HeroSection from '@/components/shadcn-studio/blocks/hero-section-01/hero-section-01';
import FeaturesSection from '@/components/eemi/features-section';
import ProgramsSection from '@/components/eemi/programs-section';

const navigationData = [
  { title: 'Programs', href: '#' },
  { title: 'Executive Courses', href: '#' },
  { title: 'About', href: '#' },
  { title: 'Admissions', href: '#' },
  { title: 'Student life', href: '#' },
  { title: 'Partners', href: '#' },
];
---

<Layout title="EEMI - European Entrepreneurship And Management Institute">
  <Header client:load navigationData={navigationData} />
  <main>
    <HeroSection client:load />
    <FeaturesSection client:load />
    <ProgramsSection client:load />
  </main>
</Layout>
